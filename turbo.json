{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "storybook": {
      "dependsOn": ["^storybook"]
    },
    "build-storybook": {
      "dependsOn": ["^build-storybook"]
    },
    "generate-api": {
      "cache": false
    },
    "lint:ts": {
      "dependsOn": ["^lint:ts"]
    }
  }
}
